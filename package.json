{
  "name": "readme-metric-reporter-express",
  "description": "Report Express.js usage metrics to README.com",
  "version": "0.0.0",
  "homepage": "https://github.com/igrek8/readme-metric-reporter-express#readme",
  "author": "Igor Korchagin (https://github.com/igrek8)",
  "license": "MIT",
  "keywords": [
    "api",
    "metrics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/igrek8/readme-metric-reporter-express"
  },
  "bugs": {
    "url": "https://github.com/igrek8/readme-metric-reporter-express/issues"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "runkitExampleFilename": "./docs/runkit.js",
  "files": [
    "./dist",
    "./docs/runkit.js"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "compile": "tsc --project ./tsconfig.build.json",
    "watch": "tsc --watch --project ./tsconfig.build.json",
    "build": "npm run clean && npm run compile",
    "lint": "eslint \"**/*\"",
    "format": "prettier --check \"**/*\""
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/node": "^16.11.59",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "@types/express": "*"
  },
  "dependencies": {
    "express": "*",
    "readme-metric-reporter": "^0.0.2",
    "tslib": "*"
  }
}
